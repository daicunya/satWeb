{{#dataList}}
  <ul class="course-list">
    {{#class}}
      <li class="course-item">
        <a href=./course-details.html?id={{id}}>
          <div class="course-img">
            <img src={{pic}} alt="课程图片">
          </div>
          <div class="course-text">
            <h2>{{cate}}</h2>
            <p><span>开班时间:</span>{{classTime}}</p>
            <p class="details bColor">查看详情</p>
          </div>
        </a>
      </li>
    {{/class}}
  </ul>
{{/dataList}}