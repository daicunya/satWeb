{{#dataList}}
  <li class="teacher-item">
    <a href="./teacher-details.html?id={{id}}">
      <div class="teacher-img">
        <img src="http://www.thinkusat.com{{pic}}" alt="图片">
      </div>
      <div class="teacher-wrap">
        <h3>{{name}}</h3>
        <p><span>主讲:</span>{{subject}}</p>
        <div class="cnt">{{introduction}}</div>
      </div>
    </a>
  </li>
{{/dataList}}